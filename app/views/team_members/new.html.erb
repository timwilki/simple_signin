<% provide(:title, 'Org users') %>
<h1>Add a new team member</h1>


<div>
  <h3>Add a new team member</h3>
  <%= form_for TeamMember.new do |f| %>
  <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :first_name, class:'form-control'%>
    <%= f.label :last_name %>
    <%= f.text_field :last_name, class:'form-control'%>
    <%= f.label :email %>
    <%= f.email_field :email, class:'form-control'%>
  </div>
  <%= f.submit "Post", class: "btn btn-primary" %>
<% end %>

<%= form_tag import_team_members_path, multipart: true do %>
  <%= file_field_tag :file, class: 'upload' %>
  <%= submit_tag "Import CSV", class: "btn btn-primary" %>
<% end %>

</div>
